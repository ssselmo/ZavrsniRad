@model PartnerDonacijeBezTransportaVM
@{
    ViewData["Title"] = "Donacije bez transporta";
    Layout = "~/Views/Shared/_Partner.cshtml";
}



<div class="content-box">
    <div class="page-box">
        <h1>@ViewData["Title"]</h1>
        <table class="table table-dark" style="width:100%">
            <thead>
                <tr>

                    <th scope="col">Tip donacije</th>
                    <th scope="col">Grad donora</th>
                    <th scope="col">Grad primaoca</th>
                    <th scope="col">Obezbijedi transport</th>


                </tr>
            </thead>
            <tbody>
                @foreach (var x in Model.rows)
                {
                    <tr>
                        <td>@x.TipDonacije</td>
                        <td>@x.GradDonora</td>
                        <td>@x.GradPrimaoca</td>

                        <td><a class="btn btn-success" asp-route-donacijaId="@x.DonacijaId" asp-action="ObezbijediTransport">Prihvati</a></td>

                    </tr>
                }
            </tbody>
        </table>

    </div>
</div>



